<h1>Editing <%= @view_name %></h1>
<form action="/<%= @view_name %>" method="put" accept-charset="utf-8">
	
	<% @html_tags.each do |key, value| %>
		<p>
			<label for="<%= key %>"><%= key.capitalize %></label><input type="<%= value %>" name="<%= key.capitalize %>" value="<%%= <%= "@#{@view_name}.#{key}" %> %>" id="<%= key %>">
		</p>		
	<% end %>
	<p><input type="submit" value="Edit &rarr;"></p>
</form>